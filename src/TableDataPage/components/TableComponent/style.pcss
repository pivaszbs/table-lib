.title-wrapper
{
    overflow: hidden;

    white-space: nowrap;
    text-overflow: ellipsis;
}

.table-wrapper--resizable table
{
    width: var(--table-width);
}

.table-header td:nth-last-child(n+2)
{
    border-right: 1px solid #e8e8e8;
}

.table-body td:nth-last-child(n+2)
{
    border-right: 1px solid #e8e8e8;
}
